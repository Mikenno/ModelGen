
a = [5, 2, 3, 1, 4]
assert a == [5, 2, 3, 1, 4]
a.sort()
assert a == [1, 2, 3, 4, 5]
a.reverse()
assert a == [5, 4, 3, 2, 1]


a = [5, 2, 3, 1, 4]

b = a.copy()
b.sort()
assert b == [1, 2, 3, 4, 5]

b = a.copy()
# Order by ascending
# b.sort(func(a, b) return a > b)
b.sort((a, b) -> a > b)
assert b == [1, 2, 3, 4, 5]

b = a.copy()
# Order by descending
# b.sort(func(a, b) return a < b)
b.sort((a, b) -> a < b)
assert b == [5, 4, 3, 2, 1]


a = [
	(1, "A"),
	(3, "B"),
	(2, "C"),
	(4, "D"),

	(3, "D"),
	(3, "C"),
	(1, "B"),
	(2, "A"),

	(1, "C"),
	(3, "A"),
	(2, "B"),
	(1, "D"),

	(2, "D"),
	(4, "B"),
	(4, "A"),
	(4, "C"),
]


b = a.copy()
b.sort((a, b) -> a[1] > b[1])
b.sort((a, b) -> a[0] > b[0])

assert b == [
	(1, "A"), (1, "B"), (1, "C"), (1, "D"),
	(2, "A"), (2, "B"), (2, "C"), (2, "D"),
	(3, "A"), (3, "B"), (3, "C"), (3, "D"),
	(4, "A"), (4, "B"), (4, "C"), (4, "D")]


b = a.copy()
b.sort((a, b) -> a[1] < b[1])
b.sort((a, b) -> a[0] > b[0])

assert b == [
	(1, "D"), (1, "C"), (1, "B"), (1, "A"),
	(2, "D"), (2, "C"), (2, "B"), (2, "A"),
	(3, "D"), (3, "C"), (3, "B"), (3, "A"),
	(4, "D"), (4, "C"), (4, "B"), (4, "A")]


b = a.copy()
b.sort((a, b) -> a[1] > b[1])
b.sort((a, b) -> a[0] < b[0])

assert b == [
	(4, "A"), (4, "B"), (4, "C"), (4, "D"),
	(3, "A"), (3, "B"), (3, "C"), (3, "D"),
	(2, "A"), (2, "B"), (2, "C"), (2, "D"),
	(1, "A"), (1, "B"), (1, "C"), (1, "D")]


b = a.copy()
b.sort((a, b) -> a[1] < b[1])
b.sort((a, b) -> a[0] < b[0])

assert b == [
	(4, "D"), (4, "C"), (4, "B"), (4, "A"),
	(3, "D"), (3, "C"), (3, "B"), (3, "A"),
	(2, "D"), (2, "C"), (2, "B"), (2, "A"),
	(1, "D"), (1, "C"), (1, "B"), (1, "A")]


b = a.copy()
b.sort((a, b) -> a[0] > b[0])
b.sort((a, b) -> a[1] > b[1])

assert b == [
	(1, "A"), (2, "A"), (3, "A"), (4, "A"),
	(1, "B"), (2, "B"), (3, "B"), (4, "B"),
	(1, "C"), (2, "C"), (3, "C"), (4, "C"),
	(1, "D"), (2, "D"), (3, "D"), (4, "D")]


b = a.copy()
b.sort((a, b) -> a[0] < b[0])
b.sort((a, b) -> a[1] > b[1])

assert b == [
	(4, "A"), (3, "A"), (2, "A"), (1, "A"),
	(4, "B"), (3, "B"), (2, "B"), (1, "B"),
	(4, "C"), (3, "C"), (2, "C"), (1, "C"),
	(4, "D"), (3, "D"), (2, "D"), (1, "D")]


b = a.copy()
b.sort((a, b) -> a[0] > b[0])
b.sort((a, b) -> a[1] < b[1])

assert b == [
	(1, "D"), (2, "D"), (3, "D"), (4, "D"),
	(1, "C"), (2, "C"), (3, "C"), (4, "C"),
	(1, "B"), (2, "B"), (3, "B"), (4, "B"),
	(1, "A"), (2, "A"), (3, "A"), (4, "A")]


b = a.copy()
b.sort((a, b) -> a[0] < b[0])
b.sort((a, b) -> a[1] < b[1])

assert b == [
	(4, "D"), (3, "D"), (2, "D"), (1, "D"),
	(4, "C"), (3, "C"), (2, "C"), (1, "C"),
	(4, "B"), (3, "B"), (2, "B"), (1, "B"),
	(4, "A"), (3, "A"), (2, "A"), (1, "A")]
